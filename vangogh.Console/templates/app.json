	},
	{
		"title": "target",
		"content": ": {progressTarget}"
	},
	{
		"title": "failures",
		"content": ". Failures: {failuresCount}"
	},
	{
		"title": "warnings",
		"content": ". Warnings: {warningsCount}"
	}
]